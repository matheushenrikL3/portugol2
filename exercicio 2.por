programa {
  funcao inicio() {
    inteiro nota1, nota2, nota3, nota4, soma, soma2
    escreva ("Digite a nota1:")
    leia (nota1)
    escreva ("Digite a nota2:")
    leia (nota2)
    escreva ("Digite a nota3:")
    leia (nota3)
    escreva ("Digite a nota4:")
    leia (nota4)
    soma = nota1 + nota2 + nota3 + nota4
    soma2 = soma / 4
    escreva (soma2)
  }
}
